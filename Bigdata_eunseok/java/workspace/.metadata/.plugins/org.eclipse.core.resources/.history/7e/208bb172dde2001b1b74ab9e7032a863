import java.io.File;
import java.io.FileWriter;

public class Exam2 {
	public static void main(String[] args) {
		String str = "가나다라마바사abcdefg";
		String path = "text2.txt";
		
		try {
			FileWriter fw = new FileWriter(path);
			fw.write(str);
			fw.close();
		}catch(Exception e) {
			e.printStackTrace();
		}
	}
}
