<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:c="http://www.springframework.org/schema/c"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="favoriteFood" class="anno2.Food" c:foodName="스파게티" c:foodPrice="7500"/>
	<bean id="hateFood" class="anno2.Food" c:foodName="보신탕" c:foodPrice="15000"/>
	<bean id="myfood" class="anno2.MyFoodMgr" p:favoriteFood-ref="favoriteFood" p:hateFood-ref="hateFood"/>
</beans>
