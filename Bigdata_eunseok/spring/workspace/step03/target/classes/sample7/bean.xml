<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="developer" class="sample7.Developer">
		<constructor-arg value="홍길동"/>
		<constructor-arg value="150000"/>
		<property name="dept" value="개발팀"/>
	</bean>
	
	<bean id="Engineer" class="sample7.Engineer">
		<constructor-arg value="이영희"/>
		<constructor-arg value="200000"/>
		<property name="dept" value="기술팀"/>
	</bean>
</beans>
